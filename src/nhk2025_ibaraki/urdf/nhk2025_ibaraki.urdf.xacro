  <link name="l_box_arm">
    <inertial>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <mass value="0.000489"/>
      <inertia ixx="0.173930" ixy="-0.000562" ixz="0.005498" iyy="0.182804" iyz="0.007761" izz="0.011761"/>
    </inertial>
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
