      <inertia ixx="0.001619" ixy="0.000140" ixz="0.000021" iyy="0.001570" iyz="0.000035" izz="0.001710"/>
    </inertial>
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="package://nhk2025_ibaraki/meshes/r_box_arm_height.stl"/>
      </geometry>
      <material name="#ffffff"/>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0"/>
