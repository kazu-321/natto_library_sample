  </joint>

  <link name="l_box_arm_strong">
    <inertial>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <mass value="0.000129"/>
      <inertia ixx="0.001302" ixy="-0.001162" ixz="0.000067" iyy="0.008836" iyz="0.000055" izz="0.009058"/>
