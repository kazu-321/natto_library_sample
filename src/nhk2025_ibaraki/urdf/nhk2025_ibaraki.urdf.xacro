      </geometry>
    </collision>
  </link>

  <joint name="r_box_arm_to_r_box_arm_height" type="prismatic">
    <origin xyz="-0.1 0.045 0.0" rpy="0.0 0.0 0.0"/>
    <limit effort="1000.0" lower="0" upper="1.0" velocity="0.5"/>
    <axis xyz = "0 0 1"/>
    <parent link="r_box_arm"/>
    <child link="r_box_arm_height"/>
  </joint>

  <link name="r_box_arm_height">
    <inertial>
      <origin xyz="0 0 0" rpy="0 0 0"/>
