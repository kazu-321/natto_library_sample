    <limit effort="1000.0" lower="0" upper="0.3" velocity="0.5"/>
    <axis xyz = "-1 0 0"/>
    <parent link="r_box_arm_height"/>
    <child link="r_box_arm_strong"/>
  </joint>

  <link name="r_box_arm_strong">
