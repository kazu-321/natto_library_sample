    </collision>
  </link>

  <joint name="c_nhk2025b_to_c_E_arm" type="revolute">
    <origin xyz="-0.225 0.0 0.225" rpy="0.0 0.0 0.0"/>
    <axis xyz="0 -1 0"/>
    <parent link="c_nhk2025b"/>
    <child link="c_E_arm"/>
    <limit lower="0" upper="3.14159" effort="0" velocity="0"/>
  </joint>

  <link name="c_E_arm">
    <inertial>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <mass value="0.001189"/>
      <inertia ixx="0.004958" ixy="-0.000229" ixz="0.006035" iyy="0.022369" iyz="0.000083" izz="0.017905"/>
    </inertial>
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
