<?xml version="1.0"?>
<robot name="nhk2025_ibaraki"  xmlns:xacro="http://ros.org/wiki/xacro">
  <!-- ==================== -->
  <!-- Parameters           -->
  <!-- ==================== -->
  <xacro:property name="wheel_radius" value="0.05"/>
  <xacro:property name="wheel_width"  value="0.02"/>
  <xacro:property name="color_alpha" value="1.0"/>

  <!-- ==================== -->
  <!-- Base link             -->
  <!-- ==================== -->
  <link name="base_link"/>

  <!-- ==================== -->
  <!-- Wheel module macro    -->
  <!-- ==================== -->
  <xacro:macro name="swerve" params="wheel_base_name wheel_name x y">

    <!-- wheel_base link -->
    <link name="${wheel_base_name}_link" />

    <!-- wheel_base joint -->
    <joint name="${wheel_base_name}" type="continuous">
      <parent link="base_link"/>
      <child link="${wheel_base_name}_link"/>
      <origin xyz="${x} ${y} ${wheel_radius}" rpy="0 0 0"/>
      <axis xyz="0 0 1"/>
    </joint>

    <!-- wheel link -->
    <link name="${wheel_name}_link">
      <visual>
        <geometry>
          <cylinder radius="${wheel_radius}" length="${wheel_width}"/>
        </geometry>
        <origin rpy="1.5708 0 0"/>
        <material name="black">
          <color rgba="0.1 0.1 0.1 ${color_alpha}"/>
        </material>
      </visual>
    </link>

    <!-- wheel joint -->
    <joint name="${wheel_name}" type="continuous">
      <parent link="${wheel_base_name}_link"/>
      <child link="${wheel_name}_link"/>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <axis xyz="0 1 0"/>
    </joint>

  </xacro:macro>

  <!-- ==================== -->
  <!-- Wheel instances       -->
  <!-- ==================== -->
  <xacro:swerve wheel_base_name="swerve_wheel_base_0" wheel_name="swerve_wheel_0" x=" 0.23"  y=" 0.33"/>
  <xacro:swerve wheel_base_name="swerve_wheel_base_1" wheel_name="swerve_wheel_1" x="-0.23"  y=" 0.33"/>
  <xacro:swerve wheel_base_name="swerve_wheel_base_2" wheel_name="swerve_wheel_2" x="-0.23"  y="-0.33"/>
  <xacro:swerve wheel_base_name="swerve_wheel_base_3" wheel_name="swerve_wheel_3" x=" 0.23"  y="-0.33"/>

  <!-- ==================== -->
  <!-- Lidar Front          -->
  <!-- ==================== -->
  <link name="lidar_front"/>
  <joint name="base_to_lidar_front" type="fixed">
    <parent link="base_link"/>
    <child link="lidar_front"/>
    <origin xyz="0.23 0 0" rpy="0 0 0"/>
  </joint>

  <!-- ==================== -->
  <!-- Lidar Rear           -->
  <!-- ==================== -->
  <link name="lidar_rear"/>
  <joint name="base_to_lidar_rear" type="fixed">
    <parent link="base_link"/>
    <child link="lidar_rear"/>
    <origin xyz="-0.23 -0.23 0" rpy="0 0 3.1415926535"/> <!-- rpy = 0 0 pi -->
  </joint>

  <!-- material color setting -->
  <material name="#ffffff">
    <color rgba="1.000 1.000 1.000 1.0"/>
  </material>

  <joint name="base_link_to_c_nhk2025b" type="fixed">
    <origin xyz="0 0 0" rpy="0.0 0.0 0.0"/>
    <parent link="base_link"/>
    <child link="c_nhk2025b"/>
  </joint>

  <link name="c_nhk2025b">
    <inertial>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <mass value="0.008084"/>
      <inertia ixx="0.493737" ixy="-0.002906" ixz="-0.092411" iyy="0.367144" iyz="-0.000025" izz="0.437208"/>
    </inertial>
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="package://nhk2025_ibaraki/meshes/c_nhk2025b.stl"/>
      </geometry>
      <material name="#ffffff"/>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="package://nhk2025_ibaraki/meshes/c_nhk2025b.stl"/>
      </geometry>
    </collision>
  </link>

  <joint name="c_nhk2025b_to_r_box_arm" type="revolute">
    <origin xyz="0.15 -0.35 0.065" rpy="0.0 0.0 0.0"/>
    <axis xyz="0 0 1"/>
    <parent link="c_nhk2025b"/>
    <child link="r_box_arm"/>
    <limit lower="0" upper="1.570796" effort="0" velocity="0"/>
  </joint>

  <link name="r_box_arm">
    <inertial>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <mass value="0.000489"/>
      <inertia ixx="0.034793" ixy="-0.000064" ixz="0.000628" iyy="0.035137" iyz="-0.000889" izz="0.024262"/>
    </inertial>
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="package://nhk2025_ibaraki/meshes/r_box_arm.stl"/>
      </geometry>
      <material name="#ffffff"/>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
