      </geometry>
      <material name="#ffffff"/>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="package://nhk2025_ibaraki/meshes/l_box_arm.stl"/>
      </geometry>
    </collision>
  </link>

  <joint name="l_box_arm_to_l_box_arm_height" type="prismatic">
    <origin xyz="-0.1 -0.045 0.0" rpy="0.0 0.0 0.0"/>
    <limit effort="1000.0" lower="0" upper="1.0" velocity="0.5"/>
    <axis xyz = "0 0 1"/>
    <parent link="l_box_arm"/>
    <child link="l_box_arm_height"/>
  </joint>

  <link name="l_box_arm_height">
    <inertial>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <mass value="0.000241"/>
      <inertia ixx="0.001837" ixy="0.000403" ixz="0.000061" iyy="0.001817" iyz="-0.000102" izz="0.002491"/>
    </inertial>
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
